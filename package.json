{
  "name": "cms-egg",
  "version": "1.0.0",
  "description": "基于eggjs的node service，用来做前后端分离的后台管理系统的后端服务。",
  "private": true,
  "dependencies": {
    "egg": "^2.2.1",
    "egg-cors": "^2.0.0",
    "egg-jwt": "^3.0.0",
    "egg-mongoose": "^2.2.1",
    "egg-mysql": "^3.0.0",
    "egg-scripts": "^2.5.0",
    "egg-static": "^2.1.1",
    "egg-view-ejs": "^2.0.0",
    "nodemailer": "^4.6.7",
    "request": "^2.87.0",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.0.0",
    "egg-bin": "^4.3.5",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.14.0",
    "eslint": "^4.11.0",
    "eslint-config-egg": "^6.0.0",
    "webstorm-disable-index": "^1.2.0"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=cms",
